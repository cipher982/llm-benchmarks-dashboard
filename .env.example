# Connection URIs
MONGODB_URI=mongodb://username:password@host:port/database?authSource=database

# CORS Configuration
DISABLE_CORS=true

# Model Management Configuration
USE_DATABASE_MODELS=true

# Admin Configuration
ADMIN_API_KEY=your-admin-key-here

# Port exposure (set for local development, remove for production)
# For local development: EXPOSE=3000
# For production: remove this line entirely
EXPOSE=3000